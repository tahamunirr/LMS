package lms;

public class course
{
	int courseId;
	int size;
	String Name;
	int creditHours;
	String SecName;
	
	public course(String n, String s, int c , int i)
	{
		courseId =i;
		Name=n;
		SecName = s;
		creditHours=c;
	}
	
	public course()
	{
		courseId =0;
		Name="\0";
		SecName = "\0";
		creditHours=0;
	}
}
